\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid     1 myLogger = \{\par
    2     _fila : [],\par
    3 \par
    4     _ocupado : false,\par
    5 \par
    6     _logWriter : null,\par
    7 \par
    8     setLogFile : function(arquivo) \{\par
    9         myLogger._logFile = arquivo;\par
   10     \},\par
   11 \par
   12     setLogWriter : function(writer) \{\par
   13         var me = this;\par
   14 \par
   15         // inicia o monitor\par
   16         setInterval(function() \{\par
   17             myLogger._monitoraFila();\par
   18         \}, 300);\par
   19 \par
   20         myLogger._logWriter = writer;\par
   21         myLogger._logWriter.onwriteend = function(e) \{\par
   22             me._ocupado = false;\par
   23         \};\par
   24         myLogger._logWriter.onerror = function(e) \{\par
   25             console.log('Erro de escrita do log: ' + e.message);\par
   26         \};\par
   27     \},\par
   28 \par
   29     _monitoraFila : function() \{\par
   30         if (myLogger._fila.length > 0 && !myLogger._ocupado) \{\par
   31             myLogger._ocupado = true;\par
   32             var strVO = myLogger._fila.slice();\par
   33             myLogger._fila = [];\par
   34 //          while (myLogger._fila.length > 0)\{\par
   35 //              str.push(myLogger._fila.shift());\par
   36 //              if (myLogger._fila.length > 1)\{\par
   37 //                  str += "\\n";\par
   38 //              \}\par
   39 //          \}\par
   40             myLogger._internalWrite(strVO);\par
   41         \}\par
   42     \},\par
   43 \par
   44     log : function(str) \{\par
   45         myLogger._fila.push("[" + device.uuid + "] [" + (new Date()) + "] " + str + "\\n"); //log moment\par
   46     \},\par
   47 \par
   48     /**\par
   49      * @param \{fileEntry\}\par
   50      *            arquivo para leitura\par
   51      * @param \{function\}\par
   52      *            cb callback que recebe o resultado da leitura do arquivo de log\par
   53      */\par
   54     read : function(arquivo, cb) \{\par
   55         arquivo.file(function(file) \{\par
   56             var reader = new FileReader();\par
   57             reader.onloadend = function(e) \{\par
   58                 cb(this.result);\par
   59             \};\par
   60             reader.readAsText(file);\par
   61         \});\par
   62     \},\par
   63 \par
   64     _internalWrite : function(strVO) \{\par
   65         //var log = "[" + device.uuid + "] [" + (new Date()) + "] " + str + "\\n";\par
   66         str = "";\par
   67         while(strVO.length > 0)\{\par
   68             str += "[" + (new Date()) + "]" + strVO.shift(); //write moment\par
   69             if (strVO.length > 1)\{\par
   70                 str += "\\n";\par
   71             \}\par
   72         \}\par
   73         \par
   74         var blob = new Blob([ str ], \{\par
   75             type : 'text/plain'\par
   76         \});\par
   77         var me = this;\par
   78         try \{\par
   79             me._logWriter.seek(me._logWriter.length);\par
   80             me._logWriter.write(blob);\par
   81             console.log(str);\par
   82         \} catch (e) \{\par
   83             console.log(e.message);\par
   84         \}\par
   85     \}\par
   86 \par
   87 \};\par
}
