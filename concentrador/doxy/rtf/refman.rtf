  186      */\par
  187     private Boolean backgroundExport(Integer posto, String date) \{\par
  188         myDB database = null;\par
  189         ResultSet result;\par
  190         FileWriter writer;\par
  191         try \{\par
  192             String qry = this.table.sql(posto);\par
  193 \par
  194             if (date != null) \{\par
  195                 if (date.equals(TODAS))\{\par
