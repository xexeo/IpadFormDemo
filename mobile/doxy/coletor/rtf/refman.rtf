   10     autentica : function(usuario, senha) \{\par
   11         var posto = null;\par
   12         var msg = null;\par
   13         if ((logins.user_tester != undefined) && (logins.user_tester.usr != undefined) && (usuario == logins.user_tester.usr)\par
