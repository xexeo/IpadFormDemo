  199                         qry += " AND dataIniPesq like '" + date + "%';";\par
  200                     \} else \{\par
  201                         qry += " AND data like '" + date + "%';";\par
  202                     \}\par
  203                 \}\par
  204                 \par
  205             \}\par
  206 \par
  207             database = myDB.getInstance();\par
  208             database.openTransaction();\par
  209             database.setStatement();\par
  210             result = database.executeQuery(qry);\par
  211 \par
  212             // writer = new FileWriter(file);\par
  213             writer = new FileWriter(tmpFile);\par
  214             writer.write(jsonBuilder.build(result));\par
  215             result.close();\par
  216             writer.close();\par
