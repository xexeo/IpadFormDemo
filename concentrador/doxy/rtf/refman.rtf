  218             zipper.zipIt();\par
  219             database.executeStatement("UPDATE " + this.table.toString() + " SET enviado=1 WHERE enviado=0;");\par
  220             database.commit();\par
  221             tmpFile.delete();\par
  222         \} catch (JSONException je) \{\par
