   25 public class LoginController \{\par
   26 \par
   27     private JSONObject loginsJson;                                              ///< Objeto com os identificadores dos postos, suas respectivas senhas e outras informa\'E7\'F5es\par
   28     private Map<String, Map<String, String>> logins;                            ///< Objeto com logins e informa\'C3\'A7o\'E1\'BA\'BDs dos postos\par
   29     private static Logger logger = LogManager.getLogger(LoginController.class); \par
   30     private static int LEN_LOGIN_USER = 3;                                      ///< Comprimento do login do usu\'E1rio\par
   31 \par
   32     /**\par
   33      * Construtor que usa o arquivo de logins padr\'E3o.\par
   34      */\par
   35     public LoginController() \{\par
   36         this("/logins/users.json");\par
   37     \}\par
   38 \par
   39     /**\par
   40      * Construtor que permite configurar o caminho do arquivo de logins.\par
   41      * @param loginsFile Caminho para o arquivo de logins e senhas\par
   42      */\par
   43     public LoginController(String loginsFile) \{\par
   44         this.buildLoginsObject(loginsFile);\par
   45     \}\par
   46 \par
   47     /**\par
   48      * Constr\'F3i a representa\'E7\'E3o interna do arquivo de logins.\par
   49      * @param loginsFile Caminho para o arquivo de logins e senhas.\par
   50      */\par
   51     private void buildLoginsObject(String loginsFile) \{\par
   52         try \{\par
   53             StringBuilder result = new StringBuilder("");\par
   54             InputStream loginsFileStream = this.getClass().getResourceAsStream(loginsFile);\par
   55             Scanner scanner = new Scanner(loginsFileStream);\par
   56             while (scanner.hasNextLine()) \{\par
   57                 String line = scanner.nextLine();\par
   58                 result.append(line).append("\\n");\par
   59             \}\par
   60             scanner.close();\par
   61             loginsJson = new JSONObject(result.toString());\par
   62             loadMapLogins(loginsJson.getJSONArray("logins"));\par
   63         \} catch (Exception e) \{\par
   64             logger.error("Erro ao ler o arquivo de login", e);\par
   65             JOptionPane.showMessageDialog(null, "Erro lendo o arquivo de login." + e.getMessage());\par
   66         \}\par
   67     \}\par
   68 \par
   69     /**\par
   70      * Constr\'F3i o mapeamento das informa\'E7\'F5es de login mantendo como chave o n\'FAmero do posto (identifica\'E7\'E3o do usu\'E1rio).\par
   71      * @param arrayLoginsJson representa\'E7\'E3o interna do arquivo de logins.\par
   72      */\par
   73     private void loadMapLogins(JSONArray arrayLoginsJson) \{\par
   74         logins = new TreeMap<String, Map<String, String>>();\par
   75         for (Object objLoginJson : arrayLoginsJson) \{\par
   76             JSONObject loginJson = (JSONObject) objLoginJson;\par
   77             Map<String, String> mapInfoByLogin = new LinkedHashMap<String, String>(loginJson.length() - 1);\par
   78             String usr = loginJson.getString("usr");\par
   79             for (String key : loginJson.keySet()) \{\par
   80                 if (!key.equals("usr")) \{\par
   81                     Object value = loginJson.get(key);\par
   82                     if (value instanceof String) \{\par
   83                         mapInfoByLogin.put(key, (String) value);\par
   84                     \} else if (value instanceof Boolean) \{\par
   85                         mapInfoByLogin.put(key, Boolean.toString(loginJson.getBoolean(key)));\par
   86                     \}\par
   87                 \}\par
   88             \}\par
   89             logins.put(usr, mapInfoByLogin);\par
   90         \}\par
   91 \par
   92     \}\par
   93 \par
   94     /**\par
   95      * Valida o par usu\'E1rio e senha informados.\par
   96      * O m\'E9todo verifica se o usu\'E1rio informado \'E9 um usu\'E1rio do per\'EDdo de treinamento ou do per\'EDodo de produ\'E7\'E3o real.\par
   97      * Em ambos os casos, ajusta os valores de vari\'E1veis de configura\'E7\'E3o como _posto_, _trecho_ e se o sistema est\'E1 rodando em modo \par
   98      * de _treinamento_.\par
   99      * A funcionalidade do m\'E9todo \'E9 implementada pelos m\'E9todos privados __verificaUserPwdProducao__, __verificaUserPwdTreinamento__\par
  100      *  e __verificaUserPwd__.\par
  101      * @param user Usu\'E1rio\par
  102      * @param pwd Senha\par
  103      * @return True se o par usu\'E1rio e senha \'E9 o adequado\par
  104      */\par
  105     public boolean validateLogin(String user, String pwd) \{\par
  106         boolean isValidLogin = false;\par
  107 \par
  108         if (verificaUserPwdProducao(user, pwd)) \{\par
  109             Concentrador.posto = user;\par
  110             Concentrador.trecho = logins.get(user).get("trecho");\par
  111             isValidLogin = true;\par
  112         \} else if (verificaUserPwdTreinamento(user, pwd)) \{\par
  113             user = user.substring(0, 3);\par
  114             Concentrador.treinamento = true;\par
  115             Concentrador.posto = user;\par
  116             Concentrador.trecho = String.format("TREINAMENTO (%s)",\par
  117                     (!user.equals(Concentrador.configuration.getProperty("trainingUser")) ? logins.get(user).get("trecho")\par
  118                             : "Trecho de teste"));\par
  119             isValidLogin = true;\par
  120         \}\par
  121 \par
  122         if (isValidLogin == true) \{\par
  123             logger.info("Login" + (Concentrador.treinamento ? " de TREINAMENTO " : " ") + "do usu\'E1rio COM sucesso: " + user);\par
  124         \} else \{\par
  125             logger.info("Tentativa de login SEM sucesso para o usu\'E1rio: " + user);\par
  126         \}\par
  127         return isValidLogin;\par
  128     \}\par
  129 \par
  130     /**\par
  131      * Verifica se o usu\'E1rio e a senha informados s\'E3o um par v\'E1lido para o per\'EDodo de produ\'E7\'E3o.\par
  132      * @param user usu\'E1rio\par
  133      * @param pwd senha\par
  134      * @return true se o par usu\'E1rio e senha for v\'E1lido para o per\'EDodo de produ\'E7\'E3o.\par
  135      */\par
  136     private boolean verificaUserPwdProducao(String user, String pwd) \{\par
  137         if (user.length() == LEN_LOGIN_USER) \{\par
  138             // try \{\par
  139             // /* O IF abaixo est\'E1 comentado por n\'E3o termos confian\'E7a na data do sistema. */\par
  140             // if (Util.isInValidPeriod(new Date())) \{\par
  141             return verificaUserPwd(user, pwd);\par
  142             // \}\par
  143             // \} catch (ParseException e) \{ /* O TRY/CATCH acompanha a condi\'E7\'E3o do IF. */\par
  144             // JOptionPane.showMessageDialog(null, "Erro ao carregar datas v\'E1lidas." + e.getMessage());\par
  145             // \}\par
  146         \}\par
  147         return false;\par
  148     \}\par
  149 \par
  150     /**\par
  151      * Verifica se o usu\'E1rio e a senha informados s\'E3o um par v\'E1lido para o per\'EDodo de treinamento.\par
  152      * @param user usu\'E1rio\par
  153      * @param pwd senha\par
  154      * @return true se o par usu\'E1rio e senha for v\'E1lido para o per\'EDodo de treinamento.\par
  155      */\par
  156     private boolean verificaUserPwdTreinamento(String user, String pwd) \{\par
  157         if ((user.equals(Concentrador.configuration.getProperty("trainingUser"))\par
  158                 || user.equalsIgnoreCase(Concentrador.configuration.getProperty("trainingUser") + "T"))\par
  159                 && pwd.equals(Concentrador.configuration.getProperty("trainingPassword"))) \{\par
  160             return true;\par
  161         \} else if ((user.length() == LEN_LOGIN_USER + 1) && (Character.toUpperCase(user.charAt(LEN_LOGIN_USER)) == 'T')) \{\par
  162             // try \{\par
  163             // /* O IF abaixo est\'E1 comentado por n\'E3o termos confian\'E7a na data do sistema. */\par
  164             // if (!Util.isInValidPeriod(new Date())) \{\par
  165             return verificaUserPwd(user.substring(0, LEN_LOGIN_USER), pwd);\par
  166             // \}\par
  167             // \} catch (ParseException e) \{ /* O TRY/CATCH acompanha a condi\'E7\'E3o do IF. */\par
  168             // JOptionPane.showMessageDialog(null, "Erro ao carregar datas de treinamento." + e.getMessage());\par
  169             // \}\par
  170         \}\par
  171         return false;\par
  172     \}\par
  173 \par
  174     /**\par
  175      * Verifica se o par usu\'E1rio senha informado \'E9 v\'E1lido.\par
  176      * @param user usu\'E1rio\par
  177      * @param pwd senha\par
  178      * @return true se o par informado for v\'E1lido\par
  179      */\par
  180     private boolean verificaUserPwd(String user, String pwd) \{\par
  181         Map<String, String> loginInfos = logins.get(user);\par
  182         if (loginInfos != null) \{\par
  183             return pwd.equals(loginInfos.get("pwd"));\par
  184         \}\par
  185         return false;\par
  186     \}\par
  187 \par
  188 \}\par
