 7053     /**\par
 7054      * Monta e exibe a tabela com os dados do sum\'E1rio das pesquisas do tipo Origem/Destino j\'E1 realizadas.\par
 7055      */\par
 7056     private synchronized void relatorioOD() \{\par
 7057         odStatus.setText("Carregando dados ...");\par
 7058         Vector<String> cols = null;\par
 7059         Vector<String> rows = null;\par
 7060         DefaultTableModel model = new DefaultTableModel() \{\par
 7061             public boolean isCellEditable(int row, int column) \{\par
 7062                 return false;\par
 7063             \}\par
 7064         \};\par
 7065 \par
 7066         try \{\par
 7067             cols = Concentrador.database.fetchReportODColumns();\par
 7068             rows = Concentrador.database.fetchReportODRows(Concentrador.getPostoInt());\par
 7069         \} catch (Exception ex) \{\par
 7070             logger.error("Error ao ler dados para relatorio OD.", ex);\par
 7071         \}\par
 7072         if (cols != null) \{\par
 7073             relatorio.setAutoCreateColumnsFromModel(true);\par
 7074             model.setColumnCount(cols.size());\par
 7075             model.setColumnIdentifiers(cols);\par
 7076             model.addColumn("Total");\par
 7077             HashMap<String, Integer> totals = new HashMap<String, Integer>();\par
 7078             for (String ipad : cols) \{\par
 7079                 if (!ipad.equals("") && !ipad.equals("Total")) \{\par
 7080                     totals.put(ipad, 0);\par
 7081                 \}\par
 7082             \}\par
 7083             for (String date : rows) \{\par
 7084                 try \{\par
 7085                     Vector<String> rowData = new Vector<String>();\par
 7086                     Map<String, Integer> ipads = Concentrador.database.fetchReportODData(date,\par
 7087                             Integer.parseInt(Concentrador.posto));\par
 7088                     rowData.add(date);\par
 7089                     int total = 0;\par
 7090                     if (ipads != null) \{\par
 7091                         for (String ipad : cols) \{\par
 7092                             if (!ipad.equals("") && !ipad.equals("Total")) \{\par
 7093                                 Integer counter = ipads.get(ipad);\par
 7094                                 if (counter != null) \{\par
 7095                                     rowData.add(String.valueOf(counter));\par
 7096                                     total += counter;\par
 7097                                     int tmp = totals.get(ipad);\par
 7098                                     totals.put(ipad, tmp + counter);\par
 7099                                 \} else\par
 7100                                     rowData.add("0");\par
 7101                             \}\par
 7102                         \}\par
 7103                     \}\par
 7104                     rowData.add(String.valueOf(total));\par
 7105                     model.addRow(rowData);\par
 7106                 \} catch (Exception e) \{\par
 7107                     e.printStackTrace();\par
 7108                 \}\par
 7109             \}\par
 7110 \par
 7111             Vector<String> totalsRow = new Vector<String>();\par
 7112             totalsRow.add("Total");\par
 7113             int sumTotal = 0;\par
 7114             for (String ipad : cols) \{\par
 7115                 if (!ipad.equals("") && !ipad.equals("Total")) \{\par
 7116                     totalsRow.add(String.valueOf(totals.get(ipad)));\par
 7117                     sumTotal += totals.get(ipad);\par
 7118                 \}\par
 7119             \}\par
 7120             totalsRow.add(String.valueOf(sumTotal));\par
 7121             model.addRow(totalsRow);\par
 7122             model.fireTableStructureChanged();\par
 7123             relatorio.setModel(model);\par
 7124             relatorio.setCellSelectionEnabled(false);\par
 7125             relatorio.setIntercellSpacing(new Dimension(0, 2));\par
 7126             odStatus.setText("");\par
 7127         \}\par
 7128     \}\par
